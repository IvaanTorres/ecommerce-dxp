{
  "name": "ecommerce",
  "version": "1.0.0",
  "description": "Full-Stack Monorepo - React (TS) + Symfony + GraphQL Support",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "docker:db": "docker-compose down && docker-compose up",
    "release": "standard-version --no-verify",
    "release:patch": "standard-version --no-verify --release-as patch",
    "release:minor": "standard-version --no-verify --release-as minor",
    "release:major": "standard-version --no-verify --release-as major",
    "release:dry": "standard-version --dry-run --no-verify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IvaanTorres/ecommerce.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/IvaanTorres/ecommerce/issues"
  },
  "homepage": "https://github.com/IvaanTorres/ecommerce#readme",
  "devDependencies": {
    "standard-version": "^9.5.0"
  }
}
